model UsuarioSistema {
    id          String      @id @default(uuid(7))
    email       String
    password    String
    tipoUsuario TipoUsuario

    funcionario Funcionario?
    cliente     Cliente?
    tarefas     Tarefa[]

    sessao Sessao?

    @@unique([email])
    @@schema("UsuarioSistema")
}
