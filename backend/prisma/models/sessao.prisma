model Sessao {
    id        Int      @id @default(autoincrement())
    token     String   @unique
    expiresAt DateTime

    userId         String         @unique
    usuarioSistema UsuarioSistema @relation(fields: [userId], references: [id])

    @@index([userId])
    @@map("sessao")
    @@schema("Sessao")
}
