generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
  directUrl    = env("DIRECT_URL")
  relationMode = "prisma"
  schemas      = ["Agenda", "ProcessoSeletivoEtapa", "AgendaCandidatura", "Localizacao", "Cliente", "Candidato", "Especialidade", "Formacao", "Contato", "Socio", "Empresa", "Habilidade", "Pessoa", "TriagemVaga", "UsuarioSistema", "Funcionario", "Vaga", "VagaHabilidade", "CandidaturaVaga", "Anexo", "VagaAnexo", "Beneficio", "VagaBeneficio", "Plano", "PlanoAssinatura", "public"]
}
